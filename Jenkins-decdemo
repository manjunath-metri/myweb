pipeline{
    agent any
    
    environment{
    PATH = " /opt/maven-3.3.9/bin:$PATH"
    }
    stages{
        stage("GIT checkout"){
            steps{
                git 'https://github.com/manjunath-metri/myweb.git'
                
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
                sh "mv target/*.war target/myweb.war"
            }
         }
    }
}
